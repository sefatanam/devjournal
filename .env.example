# DevJournal Environment Configuration
# Copy this file to .env and update the values

# Server Configuration
HTTP_PORT=8080
GRPC_PORT=8081
ENVIRONMENT=development

# PostgreSQL Configuration
POSTGRES_URL=postgres://devjournal:devjournal_secret@localhost:5432/devjournal?sslmode=disable

# MongoDB Configuration
MONGO_URL=mongodb://devjournal:devjournal_secret@localhost:27017
MONGO_DB=devjournal

# JWT Configuration
JWT_SECRET=your-secret-key-change-in-production

# Angular Frontend
API_URL=http://localhost:8080
WS_URL=ws://localhost:8080
GRPC_URL=http://localhost:9090
